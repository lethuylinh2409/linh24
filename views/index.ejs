<!DOCTYPE html>
<html>

<head>
  <title>Custommer3</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1>Custommer3</h1>
  <a href="form-add"><button>Add Custommer3</button></a><br><br>
  <table border="1">
    <tr>
      <th>ID</th>
      <th>EnableFCM</th>
      <th>Avatar</th>
      <th>Banner</th>
      <th>IsGetNotification</th>
      <th>IsVerified</th>
      <th>IsGetOpen</th>
      
      <th>ACTION</th>
    </tr>

    <% Custommer3s.forEach(item=> { %>
      <tr>
        <td>
          <%= item._id %>
        </td>
        <td>
          <%= item.EnableFCM %>
        </td>
        <td>
          <%= item.Avatar %>
        </td>
        <td>
          <%= item.Banner %>
        </td>
        <td>
          <%= item.IsGetNotification%>
        </td>
        <td>
          <%= item.IsVerified %>
        </td>
        <td>
          <%= item.IsGetOpen %>
        </td>
<td><a href="/form-update/<%= item._id %>">Update</a>||<a href="/form-delete/<%= item._id %>">Delete</a></td>
      </tr>
      <% }) %>
  </table>
</body>

</html>